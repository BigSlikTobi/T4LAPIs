__pycache__/
*.py[cod]
*$py.class
*.so
.Python
*.egg-info/
dist/
build/

# Virtual environments
venv/
env/
ENV/

# IDE files
.vscode/
.idea/
*.swp
*.swo

# OS files
.DS_Store
Thumbs.db

# Git
.git/
.gitignore

# Documentation
*.md

# Test files
.pytest_cache/
.coverage
htmlcov/

# Environment files (these should be provided at runtime)
.env
.env.local
.env.production

# Logs
*.log
logs/

# Node modules (if any)
node_modules/

# Temporary files
tmp/
temp/
